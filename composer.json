{
  "name" : "phossa/phossa-ecs",
  "type" : "project",
  "homepage" : "https://github.com/phossa/phossa-ecs",
  "description" : "A large scale PHP application skeleton using Phossa framework",
  "authors" : [ {
    "name" : "Hong Zhang",
    "homepage" : "https://github.com/phossa"
  } ],
  "license" : "MIT",
  "keywords" : [ "phossa", "application", "php" ],
  "require" : {
    "php" : ">=5.4.0",
    "fxp/composer-asset-plugin" : "^1.1",
    "phossa/phossa" : "dev-master"
  },
  "config" : {
    "bin-dir" : "system/script",
    "platform" : {
      "php" : "5.4.0"
    }
  },
  "scripts" : {
    "post-install-cmd": [
    ],
    "post-update-cmd": [
    ]
  },
  "archive" : {
    "exclude" : [ "!vendor", "tests", "*phpunit.xml", "composer.lock" ]
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "public/vendor",
      "bower-asset-library": "public/vendor"
    }
  }
}
