{
  "name" : "phossa/phossa-ecs",
  "description" : "A large scale (ECS) application skeleton for Phossa project",
  "config" : {
    "vendor-dir" : "cloud/vendor"
  },
  "autoload": {
    "psr-0": { "Phossa\\Ecs\\": "cloud/vendor/phossa/phossa-ecs" }
  },
  "require" : {
    "php" : ">=5.4.0",
    "phossa/phossa-config" : "~1.0.4"
  },
  "license" : "MIT",
  "keywords" : [ "phossa", "ecs", "application", "php" ],
  "archive" : {
    "exclude" : [ "!vendor", "tests", "*phpunit.xml", "composer.lock" ]
  },
  "type" : "framework",
  "homepage" : "https://github.com/phossa/phossa-ecs",
  "authors" : [ {
    "name" : "Hong Zhang",
    "homepage" : "https://github.com/phossa"
  } ]
}
